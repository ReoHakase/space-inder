{
  "name": "space-inder",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "9.33.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "scaffdog": "4.1.0",
    "turbo": "2.5.5"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "volta": {
    "node": "18.20.8"
  },
  "nextBundleAnalysis": {
    "budget": 358400,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 0,
    "showDetails": true
  }
}